{
  "name": "porton-health-kiosk-backend",
  "version": "1.0.0",
  "description": "Backend for Porton Health Kiosk",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-porton-health-kiosk-backend",
    "stop": "egg-scripts stop --title=egg-server-porton-health-kiosk-backend",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean"
  },
  "dependencies": {
    "date-fns": "^2.12.0",
    "egg": "^2.6.1",
    "egg-cors": "^2.2.3",
    "egg-scripts": "^2.6.0",
    "mongoose": "^5.9.9",
    "sm-crypto": "^0.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/mongoose": "^5.7.10",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "standard": "^14.3.3",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "lm902<i@lm902.cn>(https://github.com/lm902/)",
  "license": "UNLICENSED"
}
